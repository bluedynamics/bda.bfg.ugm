<tal:ajax xmlns:ajax="http://namesspaces.bluedynamics.eu/ajax" omit-tag="True">
  
  <div class="columnlisting">
    
    <ul>
      <tal:item repeat="item context.items">
        <li ajax:target="${item['target']}">
          <div class="left li_trigger"
               tal:content="item['left']">left</div>
          <div class="middle li_trigger"
               tal:content="item['right']">right</div>
          <div class="right">
            actions goes here
          </div>
        </li>
      </tal:item>
    </ul>
  
  </div>
</tal:ajax>