<properties>
  <uri>ldap://127.0.0.1:12345</uri>
  <user>cn=Manager,dc=my-domain,dc=com</user>
  <password>secret</password>
  <users_dn>ou=users300,dc=my-domain,dc=com</users_dn>
  <users_scope>0</users_scope>
  <users_query></users_query>
  <users_attrmap>
    <elem>
      <key>User ID</key>
      <value>uid</value>
    </elem>
    <elem>
      <key>Password</key>
      <value>userPassword</value>
    </elem>
    <elem>
      <key>Name</key>
      <value>cn</value>
    </elem>
    <elem>
      <key>Surname</key>
      <value>sn</value>
    </elem>
    <elem>
      <key>Email Address</key>
      <value>mail</value>
    </elem>
  </users_attrmap>
  <groups_dn>ou=groups,dc=example,dc=com</groups_dn>
  <groups_scope>0</groups_scope>
  <groups_query></groups_query>
</properties>
