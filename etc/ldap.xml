<properties>
  <uri>ldap://127.0.0.1:12345</uri>
  <user>cn=Manager,dc=my-domain,dc=com</user>
  <password>secret</password>
  <users_dn>ou=users300,dc=my-domain,dc=com</users_dn>
  <users_scope>1</users_scope>
  <users_query></users_query>
  <users_object_classes>
    <item>inetOrgPerson</item>
  </users_object_classes>
  <users_attrmap>
    <elem>
      <key>rdn</key>
      <value>uid</value>
    </elem>
    <elem>
      <key>id</key>
      <value>uid</value>
    </elem>
    <elem>
      <key>login</key>
      <value>uid</value>
    </elem>
  </users_attrmap>
  <groups_dn>ou=groups,dc=example,dc=com</groups_dn>
  <groups_scope>0</groups_scope>
  <groups_query></groups_query>
  <users_form_attrmap>
    <elem>
      <key>id</key>
      <value>Id</value>
    </elem>
    <elem>
      <key>userPassword</key>
      <value>Password</value>
    </elem>
    <elem>
      <key>cn</key>
      <value>Name</value>
    </elem>
    <elem>
      <key>sn</key>
      <value>Surname</value>
    </elem>
    <elem>
      <key>mail</key>
      <value>Email</value>
    </elem>
  </users_form_attrmap>
</properties>
